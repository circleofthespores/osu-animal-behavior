Title = {Polymorphism and division of labour in a socially complex ant:
   neuromodulation of aggression in the Australian weaver ant,
   Oecophylla smaragdina},
Journal = {PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES},
Year = {2015},
