@article{ WOS:000539647200010,
Author = {Chiu, Ming-Chung and Wu, Wen-Jer and Lai, Li-Chuan},
Title = {Carriers and cutters: size-dependent caste polyethism in the tropical
   fire ant (Solenopsis geminata)},
Journal = {BULLETIN OF ENTOMOLOGICAL RESEARCH},
Year = {2020},
Volume = {110},
Number = {3},
