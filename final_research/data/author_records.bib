@article{ WOS:000188965400002,
Author = {O'Donnell, S and Donlan, NA and Jones, TA},
Title = {Mushroom body structural change is associated with division of labor in
   eusocial wasp workers (Polybia aequatorialis, Hymenoptera:
   Vespidae)},
Journal = {NEUROSCIENCE LETTERS},
