@article{ WOS:000168710200018,
Author = {O'Donnell, S},
Title = {Worker biting interactions and task performance in a swarm-founding
   eusocial wasp (Polybia occidentalis, Hymenoptera: Vespidae)},
Journal = {BEHAVIORAL ECOLOGY},
