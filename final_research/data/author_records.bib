@article{ WOS:A1994QN26900007,
Author = {GIANNOTTI, E and MACHADO, VLL},
Title = {LONGEVITY, LIFE TABLE AND AGE POLYETHISM IN POLISTES-LANIO-LANIO
   (HYMENOPTERA, VESPIDAE), A PRIMITIVE EUSOCIAL WASP},
Journal = {JOURNAL OF ADVANCED ZOOLOGY},
Year = {1994},
Volume = {15},
Number = {2},
Pages = {95-101},
Month = {DEC},
Abstract = {Average life-span of adult wasps of Polistes lanio lanio was : dominant
   females 209.15 days, associated foundresses 69.35 days, subordinate
   females 28.28 days and males 10.52 days. A life table of subordinate
   females reveals high rates of mortality since the second week of life in
   decorrence of the starting of foraging activities. Long-lived wasps were
   the highest females in the social hierarchy of the colonies but below
   the dominant female. There are not an evident definitive division of
   tasks in different age intervals. This lacking of specialization
   presented by these wasps results in a behavioural plasticity that
   permits every individual could perform all tasks in the nest depending
   on the colonial necessities.},
